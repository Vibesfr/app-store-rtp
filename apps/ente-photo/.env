# Database Configuration
ENTE_DB_PASSWORD=your_secure_database_password

# S3 Storage Configuration
ENTE_S3_ACCESS_KEY=your_s3_access_key
ENTE_S3_SECRET_KEY=your_s3_secret_key

# JWT Configuration
ENTE_JWT_SECRET=your_jwt_secret_key

# Admin Account (configured during installation)
ENTE_ADMIN_EMAIL=admin@example.com
ENTE_ADMIN_PASSWORD=your_admin_password

# Registration Settings
ENTE_SIGNUP_DISABLED=false

# SMTP Configuration (optional)
ENTE_SMTP_HOST=smtp.gmail.com
ENTE_SMTP_PORT=587
ENTE_SMTP_USERNAME=your_email@gmail.com
ENTE_SMTP_PASSWORD=your_email_password
ENTE_SMTP_FROM=noreply@yourdomain.com
